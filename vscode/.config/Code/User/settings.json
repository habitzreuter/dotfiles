{
  "security.workspace.trust.enabled": false,
  "editor.minimap.enabled": false,
  "window.titleBarStyle": "custom",
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "-auxdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
  ],
  "latex-workshop.latex.outDir": "%DIR%/.build/",
  "editor.wordWrap": "on"
}